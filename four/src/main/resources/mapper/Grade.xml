<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">  

 <mapper namespace="dao.GradeDAO">
     <!-- 查找 -->
     <select id="findByid" parameterType="java.lang.String" resultType="Grade">
     select * from grade where eid = #{0}  and sid = #{1}
     </select>
     <insert id="add" parameterType="Grade">    
        insert into grade(eid,sid,grade,rightpercent)   
        values(#{eid},#{sid},#{grade},#{rightpercent})    
    </insert>
    <delete id="delete" parameterType="java.lang.String">    
        delete from grade where eid= #{0}
    </delete>
 </mapper>